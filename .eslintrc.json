{
	"parser": "babel-eslint",
	"env": {
		"browser": true,
		"commonjs": true,
		"es2020": true,
		"node": true
	},
	"extends": "eslint:recommended",
	"parserOptions": {
		"ecmaVersion": 6
	},
	"rules": {
		"semi": ["error", "always"],
		"quotes": ["error", "double"],
		"no-unused-vars": [
			"warn",
			{
				"vars": "local",
				"args": "after-used",
				"ignoreRestSiblings": false,
				"caughtErrors": "none"
			}
		]
	}
}
